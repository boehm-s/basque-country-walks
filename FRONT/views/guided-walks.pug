doctype html
html
  head
    title Basque Country Walks - Guided Walks
    link(href="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/css/materialize.min.css", rel="stylesheet")
    script(src="https://code.jquery.com/jquery-3.3.1.min.js", integrity="sha256-FgpCb/KJQlLNfOu91ta32o/NMZxltwRo8QtmkMRdAu8=", crossorigin="anonymous")
    script(src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js")
    link(href="https://fonts.googleapis.com/icon?family=Material+Icons", rel="stylesheet")
    link(href="/css/guided-walks.css", rel="stylesheet")

  body
    include includes/header.pug
    section#search-walks.container
        .row
            form.col.s12
                .row
                    .input-field.col.s12
                        input#search-walks-bar.validate(type="text" class="validate")
                        label(for="search-walks-bar") Search Walks

    section#list-walks.container

    script(src="/js/all.js")
    script(src="/js/guided-walks.js")
